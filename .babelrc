{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "esmodules": true
        }
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    [
      "wildcard",
      {
        "exts": ["js", "jsx"],
        "useCamelCase": true
      }
    ]
  ]
}
